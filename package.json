    "typescript": "5.2.2",
    "tailwindcss": "3.3.2",
    "autoprefixer": "10.4.12",
    "postcss": "8.4.16",
    "framer-motion": "10.12.7",
    "lucide-react": "0.287.0"
  },
  "devDependencies": {
    "eslint": "8.40.0",
    "eslint-config-next": "13.5.4",
    "prettier": "2.8.8",
    "@cloudflare/next-on-pages": "^1.13.13",
    "wrangler": "^3.20.0"
  }
}
